<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:c="http://www.springframework.org/schema/c"
       xmlns:p="http://www.springframework.org/schema/p" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-4.1.xsd">

    <context:component-scan base-package="com.stenden" />
    <context:annotation-config/>

    <bean id="loggerMessage" class="com.stenden.model.impl.LoggerMessage"/>

    <bean id="processor" class="com.stenden.model.Processor">
        <!--<constructor-arg ref="loggerMessage"/>-->
        <property name="message"  ref="loggerMessage"/>
    </bean>

</beans>